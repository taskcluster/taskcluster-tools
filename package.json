{
  "name": "taskcluster-tools",
  "version": "0.0.0-private",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "neutrino": {
    "use": ["tools-preset"],
    "config": {
      "entry": {
        "vendor": [
          "react",
          "ansi-regex",
          "ansi_up",
          "assume",
          "bootstrap",
          "change-case",
          "classnames",
          "codemirror",
          "concat-stream",
          "debug",
          "docker-exec-websocket-server",
          "durable-json-lint",
          "exorcist",
          "highlight.js",
          "hterm-umd",
          "jquery",
          "js-yaml",
          "less-loader",
          "lodash",
          "markdown-it",
          "moment",
          "redux",
          "react-helmet",
          "redux-thunk",
          "react-bootstrap",
          "react-code-mirror",
          "react-datepicker",
          "react-dom",
          "react-router-dom",
          "react-helmet",
          "react-json-inspector",
          "react-redux",
          "react-tabs",
          "react-tooltip",
          "react-treeview",
          "taskcluster-client",
          "www-authenticate",
          "xml2js",
          "qs"
        ]
      }
    }
  },
  "scripts": {
    "start": "PORT=9000 neutrino start",
    "build": "neutrino build",
    "deploy": "yarn build && s3-deploy './build/**' --cwd './build/' --region us-west-2 --bucket taskcluster-tools --gzip --cache 0",
    "test": "yarn build"
  },
  "repository": "taskcluster/taskcluster-tools",
  "dependencies": {
    "ansi-regex": "^2.0.0",
    "ansi_up": "^1.3.0",
    "assume": "^1.4.1",
    "bootstrap": "^3.3.7",
    "bufferutil": "^3.0.0",
    "change-case": "^3.0.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.22.0",
    "concat-stream": "^1.5.2",
    "debug": "^2.2.0",
    "docker-exec-websocket-server": "^1.3.1",
    "durable-json-lint": "0.0.1",
    "exorcist": "0.4.0",
    "font-awesome-svg-png": "^1.1.5",
    "highlight.js": "^9.6.0",
    "hterm-umd": "^1.0.1",
    "jquery": "^3.1.0",
    "js-yaml": "^3.7.0",
    "less-loader": "^4.0.3",
    "lodash": "^4.15.0",
    "markdown-it": "^7.0.1",
    "moment": "^2.15.0",
    "qs": "^6.4.0",
    "react": "^15.3.1",
    "react-bootstrap": "0.30.5",
    "react-code-mirror": "^3.1.0",
    "react-datepicker": "0.29.0",
    "react-dom": "^15.3.1",
    "react-helmet": "3.3.0",
    "react-json-inspector": "^7.0.0",
    "react-redux": "^4.4.5",
    "react-router-dom": "4.0.0-beta.7",
    "react-tabs": "0.7.0",
    "react-tooltip": "^3.1.8",
    "react-treeview": "0.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rison": "0.1.1",
    "shell-escape": "0.2.0",
    "slugid": "^1.1.0",
    "taskcluster-client": "^2.0.0",
    "term.js": "0.0.7",
    "utf-8-validate": "^3.0.1",
    "ws-shell": "0.1.5",
    "www-authenticate": "0.6.2",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "babel-polyfill": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "lodash.clonedeep": "^4.5.0",
    "neutrino": "^5.8.0",
    "neutrino-preset-react": "^5.8.0",
    "neutrino-middleware-env": "^5.8.0",
    "neutrino-preset-mozilla-rpweb": "^3.0.1",
    "s3-deploy": "0.6.1",
    "shebang-loader": "0.0.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
